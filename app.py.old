                             'estado': estado,
                             'fecha_desde': fecha_desde.strftime('%Y-%m-%d') if fecha_desde else '',
                             'fecha_hasta': (fecha_hasta - timedelta(days=1)).strftime('%Y-%m-%d') if fecha_hasta else '',
                             'categoria': categoria,
                             'cliente_id': cliente_id
                         },
                         current_year=current_year)

